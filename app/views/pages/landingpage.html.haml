%html{lang: "en"}
  %head
    %title Welcome to Katalyst - One stop site for wonderful gifts
    %link{href: "http://fonts.googleapis.com/css?family=Montserrat:400,700", rel: "stylesheet", type: "text/css"}/
    / IE8 support for HTML5 elements and media queries
    /[if lt IE 9]
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
  %body#page-top.index
    %header
      .container
        .row
          .col-lg-12
            %img.img-responsive{alt: "", src: "/assets/tshirtsINIT.jpg"}/
            .intro-text
              %span.name Cache Gifts
              %hr.star-light/
              %span.skills Make Amazing gifts with your instagram pictures
    %section#portfolio
      #carousel-example-generic.carousel.slide{"data-ride" => "carousel"}
        / Indicators
        %ol.carousel-indicators
          %li.active{"data-slide-to" => "0", "data-target" => "#carousel-example-generic"}
          %li{"data-slide-to" => "1", "data-target" => "#carousel-example-generic"}
          %li{"data-slide-to" => "2", "data-target" => "#carousel-example-generic"}
        / Wrapper for slides
        .carousel-inner
          .item.active
            %img{alt: "", src: "/assets/tshirtsINIT.jpg"}/
            .carousel-caption
              %p hi this is a test caption
              %h3 Rockstar t-shirts  
          .item
            %img{alt: "", src: "/assets/mugsINIT.jpg"}/
            .carousel-caption
              %h3 Pictastic mugs   
              
        / Controls
        %a.left.carousel-control{"data-slide" => "prev", href: "#carousel-example-generic", role: "button"}
          %span.glyphicon.glyphicon-chevron-left
        %a.right.carousel-control{"data-slide" => "next", href: "#carousel-example-generic", role: "button"}
          %span.glyphicon.glyphicon-chevron-right         
    %section#portfolio
      .container
